{
    "name": "lubnik2005/sharepoint-flysystem",
    "description": "A PHP Flysystem for Microsoft's Sharepoint using GraphQL",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Lubnik2005\\SharepointFlysystem\\": "src/"
        }
    },
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.4",
        "league/flysystem": "^3.0"
    },
    "require-dev": {
        "league/flysystem-adapter-test-utilities": "^3.0",
        "phpunit/phpunit": "^9.5",
        "vlucas/phpdotenv": "^5.4"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
